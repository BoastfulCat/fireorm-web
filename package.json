{
  "name": "fireorm-web",
  "version": "0.1.8",
  "description": "ORM for Web Firestore",
  "author": "BoastfulCat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BoastfulCat/fireorm-web/issues"
  },
  "homepage": "https://github.com/BoastfulCat/fireorm-web#readme",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BoastfulCat/fireorm-web.git"
  },
  "keywords": [
    "firebase",
    "firestore",
    "orm"
  ],
  "scripts": {
    "build": "tsc && tsc -p src",
    "watch": "tsc -w --incremental",
    "lint": "eslint src/**/*.ts",
    "release": "semantic-release",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "pluralize": "^8.0.0",
    "ts-object-path": "^0.1.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/pluralize": "^0.0.29",
    "firebase": "^9.8.3",
    "typescript": "^4.7.3",
    "reflect-metadata": "^0.1.13",
    "semantic-release": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-google": "^0.14.0",
    "ts-node": "^10.8.1"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ],
    "plugins": [
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator"
    ]
  },
  "files": [
    "/lib",
    "!**/*.map",
    "!**/*.spec.*",
    "!**/examples/**",
    "!**/test/**"
  ]
}
